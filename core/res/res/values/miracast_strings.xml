<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <!-- label for item that toggles Miracast -->
    <string name="global_actions_toggle_miracast_mode">Miracast wireless display</string>

    <!-- status message in phone options dialog for when miracast is going to be on -->
    <string name="global_actions_miracast_mode_cancel_connection_status">Cancel connection</string>

    <!-- status message in phone options dialog for when miracast mode is on -->
    <string name="global_actions_miracast_mode_on_status">Stop streaming</string>

    <!-- status message in phone options dialog for when miracast mode is off and no sinks are remembered -->
    <string name="global_actions_miracast_mode_off_status_no_paired">Connect to display</string>

    <!-- status message in phone options dialog for when miracast mode is off and one remembered sink -->
    <string name="global_actions_miracast_mode_off_status_single_paired">Connect to\u0020</string>

    <!-- status message in phone options dialog for when miracast mode is off and two or more than two sinks are remembered -->
    <string name="global_actions_miracast_mode_off_status_many_paired">Connect to previously paired display</string>

    <!-- status message in progress dialog for miracast connection -->
    <string name="global_actions_miracast_connection_progress_status">Connecting to the display...</string>

    <!-- status message in progress dialog for miracast sink search -->
    <string name="global_actions_miracast_sink_search_status">Searching for the display...</string>

    <!-- status message in progress dialog when miracast sink is not found while trying to connect -->
    <string name="global_actions_miracast_sink_not_found_status">Could not find the display.</string>

    <!-- status message in progress dialog when miracast connection fails -->
    <string name="global_actions_miracast_connection_failed_status">Connection failed.</string>

    <!-- status message in phone options dialog for when airplane mode is on -->
    <string name="global_actions_miracast_mode_airplane_on_status">Unavailable because Airplane mode is on</string>

    <!-- status message in phone options dialog for when wi-fi mode is off -->
    <string name="global_actions_miracast_mode_wifi_off_status">Unavailable because Wi-Fi is turned off</string>

    <!-- status message in phone options dialog for when pc2thor streaming is active -->
    <string name="global_actions_miracast_mode_pc2thor_on_status">Unavailable due to PC streaming to SHIELD</string>
</resources>
