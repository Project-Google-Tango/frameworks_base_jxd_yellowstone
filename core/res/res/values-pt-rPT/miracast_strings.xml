<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <!-- label for item that toggles Miracast -->
    <string name="global_actions_toggle_miracast_mode">Monitor sem fios Miracast</string>

    <!-- status message in phone options dialog for when miracast is going to be on -->
    <string name="global_actions_miracast_mode_cancel_connection_status">Cancelar ligação</string>

    <!-- status message in phone options dialog for when miracast mode is on -->
    <string name="global_actions_miracast_mode_on_status">Parar transmissão em fluxo</string>

    <!-- status message in phone options dialog for when miracast mode is off and no sinks are remembered -->
    <string name="global_actions_miracast_mode_off_status_no_paired">Ligar ao monitor</string>

    <!-- status message in phone options dialog for when miracast mode is off and one remembered sink -->
    <string name="global_actions_miracast_mode_off_status_single_paired">Ligar a \u0020</string>

    <!-- status message in phone options dialog for when miracast mode is off and two or more than two sinks are remembered -->
    <string name="global_actions_miracast_mode_off_status_many_paired">Ligar a monitor emparelhado previamente</string>

    <!-- status message in progress dialog for miracast connection -->
    <string name="global_actions_miracast_connection_progress_status">A ligar ao monitor..</string>

    <!-- status message in progress dialog for miracast sink search -->
    <string name="global_actions_miracast_sink_search_status">A procurar o monitor...</string>

    <!-- status message in progress dialog when miracast sink is not found while trying to connect -->
    <string name="global_actions_miracast_sink_not_found_status">Não foi possível localizar o monitor.</string>

    <!-- status message in progress dialog when miracast connection fails -->
    <string name="global_actions_miracast_connection_failed_status">Falha da ligação.</string>

    <!-- status message in phone options dialog for when airplane mode is on -->
    <string name="global_actions_miracast_mode_airplane_on_status">Indisponível porque o modo de voo está activado</string>

    <!-- status message in phone options dialog for when wi-fi mode is off -->
    <string name="global_actions_miracast_mode_wifi_off_status">Indisponível porque o Wi-Fi está desligado</string>

    <!-- status message in phone options dialog for when pc2thor streaming is active -->
    <string name="global_actions_miracast_mode_pc2thor_on_status">Indisponível devido a transmissão do PC para o SHIELD</string>
</resources>
