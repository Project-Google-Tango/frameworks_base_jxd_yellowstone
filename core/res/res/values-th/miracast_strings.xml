<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <!-- label for item that toggles Miracast -->
    <string name="global_actions_toggle_miracast_mode">หน่วยแสดงผลแบบไร้สายมิราแคสต์</string>

    <!-- status message in phone options dialog for when miracast is going to be on -->
    <string name="global_actions_miracast_mode_cancel_connection_status">ยกเลิกการเชื่อมต่อ</string>

    <!-- status message in phone options dialog for when miracast mode is on -->
    <string name="global_actions_miracast_mode_on_status">หยุดการสตรีม</string>

    <!-- status message in phone options dialog for when miracast mode is off and no sinks are remembered -->
    <string name="global_actions_miracast_mode_off_status_no_paired">เชื่อมต่อกับหน่วยแสดงผล</string>

    <!-- status message in phone options dialog for when miracast mode is off and one remembered sink -->
    <string name="global_actions_miracast_mode_off_status_single_paired">เชื่อมต่อกับ\u0020</string>

    <!-- status message in phone options dialog for when miracast mode is off and two or more than two sinks are remembered -->
    <string name="global_actions_miracast_mode_off_status_many_paired">เชื่อมต่อไปยังหน่วยแสดงผลที่จับคู่ก่อนหน้า</string>

    <!-- status message in progress dialog for miracast connection -->
    <string name="global_actions_miracast_connection_progress_status">กำลังเชื่อมต่อไปยังหน่วยแสดงผล...</string>

    <!-- status message in progress dialog for miracast sink search -->
    <string name="global_actions_miracast_sink_search_status">กำลังค้นหาหน่วยแสดงผล...</string>

    <!-- status message in progress dialog when miracast sink is not found while trying to connect -->
    <string name="global_actions_miracast_sink_not_found_status">ไม่พบหน่วยแสดงผล</string>

    <!-- status message in progress dialog when miracast connection fails -->
    <string name="global_actions_miracast_connection_failed_status">การเชื่อมต่อล้มเหลว</string>

    <!-- status message in phone options dialog for when airplane mode is on -->
    <string name="global_actions_miracast_mode_airplane_on_status">ใช้งานไม่ได้เนื่องจากเปิดโหมดใช้งานบนเครื่องบินอยู่</string>

    <!-- status message in phone options dialog for when wi-fi mode is off -->
    <string name="global_actions_miracast_mode_wifi_off_status">ใช้งานไม่ได้เนื่องจากปิด Wi-Fi อยู่</string>

    <!-- status message in phone options dialog for when pc2thor streaming is active -->
    <string name="global_actions_miracast_mode_pc2thor_on_status">ใช้งานไม่ได้เนื่องพีซีกำลังสตรีมไปยัง SHIELD</string>
</resources>
