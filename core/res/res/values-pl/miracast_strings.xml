<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <!-- label for item that toggles Miracast -->
    <string name="global_actions_toggle_miracast_mode">Bezprzewodowy monitor Miracast</string>

    <!-- status message in phone options dialog for when miracast is going to be on -->
    <string name="global_actions_miracast_mode_cancel_connection_status">Anuluj połączenie</string>

    <!-- status message in phone options dialog for when miracast mode is on -->
    <string name="global_actions_miracast_mode_on_status">Zatrzymaj przesyłanie strumieniowe</string>

    <!-- status message in phone options dialog for when miracast mode is off and no sinks are remembered -->
    <string name="global_actions_miracast_mode_off_status_no_paired">Połącz z monitorem</string>

    <!-- status message in phone options dialog for when miracast mode is off and one remembered sink -->
    <string name="global_actions_miracast_mode_off_status_single_paired">Połącz z\u0020</string>

    <!-- status message in phone options dialog for when miracast mode is off and two or more than two sinks are remembered -->
    <string name="global_actions_miracast_mode_off_status_many_paired">Połącz z powiązanym wcześniej monitorem</string>

    <!-- status message in progress dialog for miracast connection -->
    <string name="global_actions_miracast_connection_progress_status">Łączenie z monitorem...</string>

    <!-- status message in progress dialog for miracast sink search -->
    <string name="global_actions_miracast_sink_search_status">Wyszukiwanie monitora...</string>

    <!-- status message in progress dialog when miracast sink is not found while trying to connect -->
    <string name="global_actions_miracast_sink_not_found_status">Nie można znaleźć monitora.</string>

    <!-- status message in progress dialog when miracast connection fails -->
    <string name="global_actions_miracast_connection_failed_status">Połączenie nie powiodło się.</string>

    <!-- status message in phone options dialog for when airplane mode is on -->
    <string name="global_actions_miracast_mode_airplane_on_status">Niedostępne, ponieważ tryb samolotowy jest włączony</string>

    <!-- status message in phone options dialog for when wi-fi mode is off -->
    <string name="global_actions_miracast_mode_wifi_off_status">Niedostępne, ponieważ sieć Wi-Fi jest wyłączona</string>

    <!-- status message in phone options dialog for when pc2thor streaming is active -->
    <string name="global_actions_miracast_mode_pc2thor_on_status">Niedostępne, ponieważ komputer PC przesyła strumieniowo do urządzenia SHIELD</string>
</resources>
