<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <!-- label for item that toggles Miracast -->
    <string name="global_actions_toggle_miracast_mode">Miracast ሽቦአልባ ማሳያ</string>

    <!-- status message in phone options dialog for when miracast is going to be on -->
    <string name="global_actions_miracast_mode_cancel_connection_status">ግንኙነት ሰርዝ</string>

    <!-- status message in phone options dialog for when miracast mode is on -->
    <string name="global_actions_miracast_mode_on_status">መልቀቅ አቁም</string>

    <!-- status message in phone options dialog for when miracast mode is off and no sinks are remembered -->
    <string name="global_actions_miracast_mode_off_status_no_paired">ወደ ማሳያ አገናኝ</string>

    <!-- status message in phone options dialog for when miracast mode is off and one remembered sink -->
    <string name="global_actions_miracast_mode_off_status_single_paired">አገናኝ ወደ\u0020</string>

    <!-- status message in phone options dialog for when miracast mode is off and two or more than two sinks are remembered -->
    <string name="global_actions_miracast_mode_off_status_many_paired">ከዚህ ቀደም የተጣመረ ማሳያ ላይ አገናኝ</string>

    <!-- status message in progress dialog for miracast connection -->
    <string name="global_actions_miracast_connection_progress_status">ማሳያ ላይ በማገናኘት ላይ...</string>

    <!-- status message in progress dialog for miracast sink search -->
    <string name="global_actions_miracast_sink_search_status">ማሳያውን በመፈለግ ላይ...</string>

    <!-- status message in progress dialog when miracast sink is not found while trying to connect -->
    <string name="global_actions_miracast_sink_not_found_status">ማሳያውን ማግኘት አልተቻለም።</string>

    <!-- status message in progress dialog when miracast connection fails -->
    <string name="global_actions_miracast_connection_failed_status">ግንኙነት አልተሳካም።</string>

    <!-- status message in phone options dialog for when airplane mode is on -->
    <string name="global_actions_miracast_mode_airplane_on_status">አይገኝም፣ ምክንያቱም አይሮፕላን ሁነታ በርቷል</string>

    <!-- status message in phone options dialog for when wi-fi mode is off -->
    <string name="global_actions_miracast_mode_wifi_off_status">አይገኝም፣ ምክንያቱም Wi-Fi ጠፍቷል</string>

    <!-- status message in phone options dialog for when pc2thor streaming is active -->
    <string name="global_actions_miracast_mode_pc2thor_on_status">PC ወደ SHIELD እየተለቀቀስለሆነ አይገኝም</string>
</resources>
